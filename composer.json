{
    "name": "thecodingmachine/tdbm-metahydrator",
    "description": "Bindings between TDBM and thecodingmachine/metahydrator.",
    "license": "MIT",
    "authors": [
        {
            "name": "David NÃ©grier",
            "email": "d.negrier@thecodingmachine.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:dsavina/metahydrator.git"
        }
    ],
    "require": {
        "php": ">=7.1",
        "thecodingmachine/tdbm": "^5.0",
        "thecodingmachine/metahydrator": "dev-feature/db-parsers"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "TheCodingMachine\\TDBM\\MetaHydrator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheCodingMachine\\TDBM\\MetaHydrator\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
